{
    "maxlen": 1000,
    "trailing": true,
    "eqeqeq": true,
    "browser": true,
    "devel": true,
    "maxparams": 99,
    "quotmark": "single",
    "curly": true,
    "undef": true,
    "unused": true,
    "predef": [
      "require",
      "define",
      "Promise",
      "__DEBUG__",
      "__REPO__",
      "__SELF_HOSTED__",
      "__BUILD_VERSION__",
      "__FLASH_VERSION__"
    ]
}
